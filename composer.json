{
    "name": "hallewood/logsnag-php",
    "description": "A PHP SDK for the LogSnag API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dennis Prudlo",
            "email": "mail@dennisprudlo.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Hallewood\\LogSnag\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
